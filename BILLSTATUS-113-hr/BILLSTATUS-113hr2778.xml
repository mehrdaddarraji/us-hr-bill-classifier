    <summaries>
      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2013-07-22</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <text><![CDATA[<p>Child Tax Credit Integrity Preservation Act of 2013 - Amends the Internal Revenue Code to expand the identification requirements for the child tax credit to require taxpayers to provide valid identification numbers on their tax returns in addition to the names and identification numbers of each qualifying child.</p>]]></text>
          <updateDate>2013-07-22T04:00:00Z</updateDate>
          <lastSummaryUpdateDate>2013-08-13T20:19:59Z</lastSummaryUpdateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
