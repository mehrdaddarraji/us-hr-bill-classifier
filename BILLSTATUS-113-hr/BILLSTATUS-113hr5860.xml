      <billSummaries>
        <item>
          <actionDesc>Introduced in House</actionDesc>
          <versionCode>00</versionCode>
          <text><![CDATA[<p>Small Business Healthcare Relief Act of 2014 - Amends the Internal Revenue Code to treat employer payments for employee health care premiums and for amounts paid on behalf of an employee to a health reimbursement arrangement as medical expenses under an accident or health plan and thus excludible from gross income for income tax purposes.</p>]]></text>
          <actionDate>2014-12-11</actionDate>
          <name>Introduced in House</name>
          <updateDate>2014-12-11T05:00:00Z</updateDate>
          <lastSummaryUpdateDate>2015-01-14T14:47:47Z</lastSummaryUpdateDate>
        </item>
      </billSummaries>
