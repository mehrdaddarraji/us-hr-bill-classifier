    <summaries>
      <billSummaries>
        <item>
          <text><![CDATA[<p>Amends the Internal Revenue Code to authorize the Secretary of the Treasury to prescribe regulations to regulate the conduct of enrolled agents in their practice before the Internal Revenue Service (IRS). Allows properly licensed enrolled agents to use the credentials or designation as &quot;enrolled agent,&quot; &quot;EA,&quot; or &quot;E.A.&quot;</p>]]></text>
          <name>Introduced in House</name>
          <versionCode>00</versionCode>
          <lastSummaryUpdateDate>2013-06-25T19:14:22Z</lastSummaryUpdateDate>
          <actionDate>2013-06-11</actionDate>
          <updateDate>2013-06-11T04:00:00Z</updateDate>
          <actionDesc>Introduced in House</actionDesc>
        </item>
      </billSummaries>
