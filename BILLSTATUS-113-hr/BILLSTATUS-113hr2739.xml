      <item>
        <chamberCode />
        <title>To require the reallocation and auction for commercial use of the electromagnetic spectrum between the frequencies from 1755 megahertz to 1780 megahertz.</title>
        <titleType>Official Title as Introduced</titleType>
        <parentTitleType />
        <chamberName />
